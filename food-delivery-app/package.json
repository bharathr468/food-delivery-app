{
	"name": "food-delivery-app",
	"version": "0.1.0",
	"scripts": {
		"next:dev": "next dev",
		"tw:dev": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --watch",
		"dev": "run-p tw:dev next:dev",
		"build": "npx tailwindcss -i ./src/styles/tw.css -o ./src/styles/globals.css --minify && next build",
		"fmt": "npx prettier --write src",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@prisma/client": "^4.12.0",
		"@tabler/icons-react": "^2.16.0",
		"hamburger-react": "^2.5.0",
		"next": "13.3.0",
		"nprogress": "^0.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "^2.4.0"
	},
	"devDependencies": {
		"@types/node": "18.15.11",
		"@types/nprogress": "^0.2.0",
		"@types/react": "18.0.35",
		"@types/react-dom": "18.0.11",
		"eslint": "8.38.0",
		"eslint-config-next": "13.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.7",
		"prisma": "^4.12.0",
		"tailwindcss": "^3.3.1",
		"typescript": "5.0.3"
	}
